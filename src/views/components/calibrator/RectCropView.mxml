		[Event(name="COMPLETE", type="events.CameraEvent")]
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			
			protected var originalWidth : int;
			protected var originalHeight : int;
			private var trophyZero:Point;
			private var mouseZero:Point;
			
			

			

			

			
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<calibrator:SimpleDataModel id="model" />
	</fx:Declarations>
	<s:Rect width="{width}" height="{height}" >
		<s:stroke>
			<s:SolidColorStroke color="0x000000" weight="2" />
